

//User function Template for Java

class Solution 
{
    int findOnce(int arr[], int n)
    {
        // Complete this function
        
        for(int i=0;i<n;i+=2){
            if(i == n-1)
                return arr[i];
            if(arr[i] != arr[i+1])
                return arr[i];
        }
        return 0;
    }
}
